rows_to_add_1 = []
rows_to_add_2 = []
# 90000 91000
rows_to_add_1.append(['B',  -10000, 92000.0, B, 0, 0])
rows_to_add_1.append(['B',  -10000, 93000.0, B, 0, 0])
